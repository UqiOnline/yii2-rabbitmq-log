    "keywords": ["yii2", "extension", "log", "RabbitMQ"],
    "homepage": "https://github.com/liuzongquan/yii2-rabbitmq-log",
    "type": "yii2-extension",
    "license": "BSD-3-Clause",
    "authors": [
        {
            "name": "liuzongquan",
            "email": "zongquanliu2010@gmail.com"
        }
    ],
    "require": {
        "php": ">=5.3.0",
        "php-amqplib/php-amqplib": "2.6.*"
    },
    "autoload": {
        "psr-4": {
            "rabbitmq\\": "*"
        }
    }
}
